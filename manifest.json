{

  "manifest_version": 2,
  "name": "Wikify",
  "version": "1.0",

  "description": "anaddi",

  "permissions": [
    "tabs",
    "bookmarks"
  ],

  "background": {
    "scripts": ["extension.js"]
  },

  "content_scripts": [
    {
      "matches": ["*://*/"],
      "js": ["extension.js"]
    }
  ]
}
